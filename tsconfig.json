{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/contact/page.jsx", "app/components/video.jsx", "app/components/mainnav.jsx", "app/components/githubbusimate.jsx", "app/components/aboutme.jsx", "app/aboutmeinfo/buttonaboutme.tsx", "app/aboutmeinfo/page.jsx", "app/components/personalproject.jsx", "app/components/tools.jsx", "app/layout.jsx", "app/page.jsx"],
  "exclude": ["node_modules"]
}
